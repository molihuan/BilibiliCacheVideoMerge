<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <!--描边stroke-->
    <item android:state_pressed="false">
        <shape>

            <solid
                android:color="#FFFFFF"/>

            <corners
                android:radius="5dp"/>
        </shape>
    </item>


    <item android:state_pressed="true">
        <shape>

            <solid
                android:color="#FBF1ED"/>
            <corners
                android:radius="5dp"/>
        </shape>
    </item>

</selector>