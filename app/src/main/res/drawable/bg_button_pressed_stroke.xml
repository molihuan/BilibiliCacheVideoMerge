<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <!--描边stroke-->
    <item android:state_pressed="false">
        <shape>
            <stroke
                android:width="0.1dp"
                />
            <solid
                android:color="#FFFFFF"/>

            <corners
                android:radius="5dp"/>
        </shape>
    </item>


    <item android:state_pressed="true">
        <shape>
            <stroke
                android:width="0.1dp"
                />
            <solid
                android:color="#FBF1ED"/>
            <corners
                android:radius="5dp"/>
        </shape>
    </item>

</selector>